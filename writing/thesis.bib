journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {bottlenose dolphin,resilience analysis,scale-free networks,sociality},
number = {Suppl{\_}2},
pages = {S186--S188},
pmid = {14667378},
primaryClass = {cond-mat},
title = {{The emergent properties of a dolphin social network}},
url = {http://rspb.royalsocietypublishing.org/cgi/doi/10.1098/rsbl.2003.0057},
volume = {270},
year = {2003}
}
